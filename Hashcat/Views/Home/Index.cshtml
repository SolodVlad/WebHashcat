@{
    ViewData["Title"] = "Home Page";
}

<head>
    <link rel="stylesheet" href="~/css/searchByDatabase.css" asp-append-version="true" />
</head>

<section>
    <div class="container">
        <form>
            <div class="row">
                <div class="col-md-11">
                    <textarea class="form-control search-db-style" id="hashes" placeholder="Додайте свій хеш..."></textarea>
                </div>
                <div class="col-md-1 mt-3 mt-md-0">
                    <div class="d-grid">
                        <input class="btn btn-primary def-btn" id="searchPass" type="button" value="Пошук" />
                    </div>
                </div>
            </div>
            <div class="row mt-3">
                <p class="text-db"><strong>Розшифровка кольорів:  </strong> <mark style="background-color: green">  Зелений:  </mark>  Пароль знайдено,  <mark style="background-color: yellow">  Жовтий:  </mark>  Паролю нема в базі,  <mark style="background-color: red"> Червоний:  </mark>  Помилка. Необролюємий хеш.</p>
            </div>
        </form>
    </div>
</section>
